{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug push_swap (Custom Args)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/push_swap",
			"args": [
				"214",
				"3",
				"12",
				"-4",
				"1"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "Build push_swap",
			"miDebuggerPath": "/usr/bin/gdb"
		},
		{
			"name": "Debug push_swap (5 numbers)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/push_swap",
			"args": [
				"5",
				"2",
				"3",
				"1",
				"4"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "Build push_swap",
			"miDebuggerPath": "/usr/bin/gdb"
		}
	]
}